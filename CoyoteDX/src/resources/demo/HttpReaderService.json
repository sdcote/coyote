{
	"Class": "coyote.dx.Service",

	"Job": {
		"Reader": {
			"class": "HttpReader",
			"port": 80,
			"timeout": 5000,
			"endpoint": "/coyote"
		},

		"Transform": {
			"Counter": { "field": "RecNum" },
			"Guid": { "field": "ID" },
			"Timestamp": { "field": "Received" }
		},

		"Writer": {
			"class": "ConsoleWriter",
			"format": "json",
			"indent": false,
			"message": "Record [#$CurrentFrame#] received."
		}

	},

	"Job": {
		"Reader": {
			"class": "HttpReader",
			"port": 80,
			"timeout": 5000,
			"endpoint": "/roadrunner"
		},

		"Transform": {
			"Counter": { "field": "RecNum" },
			"Guid": { "field": "ID" },
			"Timestamp": { "field": "Received" }
		},

		"Writer": {
			"class": "ConsoleWriter",
			"format": "json",
			"indent": false,
			"message": "Meep! Meep!"
		}

	},

	"Job": {
		"Reader": {
			"class": "HttpReader",
			"port": 80,
			"timeout": 5000,
			"endpoint": "/acme"
		},
		"Writer": {
			"class": "ConsoleWriter",
			"format": "json",
			"indent": false
		}
	},

	"Job": {
		"Reader": {
			"class": "HttpReader",
			"port": 80,
			"timeout": 5000,
			"endpoint": "/zenith"
		},
		"Writer": {
			"class": "ConsoleWriter",
			"format": "json",
			"indent": false
		}
	},
	
	
	"Logging": { 
		"StandardOutput": { "categories": "scheduler, info, notice, warn, error, fatal" } 
	}

}
